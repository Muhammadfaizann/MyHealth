<?xml version="1.0" encoding="UTF-8" ?>
<resources>
	<color name="white_opaque">#FFFFFFFF</color>
	<color name="red_opaque">#ffC90C30</color>
	<color name="btn_pressed">#ff84D3EF</color>
  	<style name="Theme.Splash" parent="android:Theme">
  		<item name="android:windowBackground">@color/red_opaque</item>
  		<item name="android:colorBackground">@color/red_opaque</item>
    	<item name="android:windowNoTitle">true</item>
  	</style>

  	<style name="MyHealthTheme" parent="android:Theme.Holo.Light">
  		<item name="android:actionBarStyle">@style/CustomActionBar</item>
  	</style>
  	<style name="CustomActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
  		<item name="android:background">@color/white_opaque</item>
  	</style>

</resources>